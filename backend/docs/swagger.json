{
    "swagger": "2.0",
    "info": {
        "description": "Example API",
        "title": "My API",
        "contact": {},
        "version": "1.0"
    },
    "paths": {
        "/api/v1/ping": {
            "get": {
                "description": "Возвращает \"pong\" если сервер работает",
                "tags": [
                    "Ping"
                ],
                "summary": "Проверка доступности сервера",
                "responses": {
                    "200": {
                        "description": "pong",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/printer/find": {
            "get": {
                "description": "Возвращает ip принтера",
                "tags": [
                    "Printer"
                ],
                "summary": "Находит ip принтера",
                "responses": {
                    "200": {
                        "description": "ip",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/printer/print": {
            "post": {
                "description": "Отправляет текст на принтер",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Printer"
                ],
                "summary": "Отправляет текст на принтер",
                "parameters": [
                    {
                        "description": "Request body",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/handlers.PrintRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/handlers.PrintResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "handlers.PrintRequest": {
            "type": "object",
            "properties": {
                "ip": {
                    "type": "string"
                },
                "text": {
                    "type": "string"
                }
            }
        },
        "handlers.PrintResponse": {
            "type": "object",
            "properties": {
                "result": {
                    "type": "string"
                }
            }
        }
    },
    "securityDefinitions": {
        "BearerAuth": {
            "type": "apiKey",
            "name": "Authorization",
            "in": "header"
        }
    }
}